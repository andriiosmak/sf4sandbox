<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400">
            <link rel="stylesheet" href="{{ asset('themes/classic/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('themes/classic/css/styles.css') }}">
        {% endblock %}
    </head>
    <body>
        {{ include('partials/themes/classic/header.html.twig') }}
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('themes/classic/css/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('themes/classic/css/js/jquery-1.11.3.min.js') }}"></script>
        {% endblock %}
        {{ include('partials/themes/classic/footer.html.twig') }}
    </body>
</html>
